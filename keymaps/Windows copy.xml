    <keyboard-shortcut first-keystroke="alt a" />
  </action>
  <action id="Debugger.PopFrame">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="EditorToggleActions">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
  <action id="FindPrevious">
    <keyboard-shortcut first-keystroke="shift f3" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
</keymap>